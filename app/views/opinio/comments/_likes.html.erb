 <% comment = Comment.find(like_object)%>
  <div id="comment_like_<%= comment.id %>">
             <%= time_ago_in_words(comment.created_at) %> ago.
             
             <%- if current_user.likes? comment -%><%= unlike_link_for comment  %>
             <%- else -%>
             <%= like_link_for comment %>
             <%- end -%>
              
              (<%= comment.like_count %> Given)
             
             
 </div>
<h2>Sign up</h2>
<div class="span3 pull left">
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
   <%= f.fields_for :profile do |profile_form| %>
   <div class="control-group">

      <%= profile_form.label :fname %><br />
      <%= profile_form.text_field :fname %>
   </div>
   <div class="control-group">
      <%= profile_form.label :lname %><br />
      <%= profile_form.text_field :lname %>
   </div>
    <% end %>
   <div class="control-group"><%= f.label :email %><br />
  <%= f.email_field :email %></div>

 <div class="control-group"><%= f.label :password %><br />
  <%= f.password_field :password, :autocomplete => "off" %></div>

 <div class="control-group"><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>

 <div class="form-actions">
  <%= f.submit "Sign Up", :class => 'btn btn-primary'  %>
 </div>
<% end %>
</div>
<div class="span3">
<%= render :partial => "devise/shared/links" %>
</div>

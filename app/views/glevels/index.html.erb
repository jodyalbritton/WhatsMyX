<h1>Listing glevels</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @glevels.each do |glevel| %>
  <tr>
    <td><%= glevel.name %></td>
    <td><%= link_to 'Show', glevel %></td>
    <td><%= link_to 'Edit', edit_glevel_path(glevel) %></td>
    <td><%= link_to 'Destroy', glevel, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Glevel', new_glevel_path %>

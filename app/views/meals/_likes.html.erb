     <% activity = Meal.find(like_object)%>
              <div id="activity_like_<%= activity.id %>" class="activity-like-menu">
         
  
             <%= activity.like_count %> Props    
             <%- if current_user.likes? activity -%><%= unlike_link_for activity  %>
             <%- else -%>
             <%= like_link_for activity  %>
             <%- end -%>
              <a href="javascript:toggleDiv('new_comment_<%=activity.id %>');" class= "icon-comment"> Comment</a>  
              </div>